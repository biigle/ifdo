{
    "name": "biigle/ifdo-parser",
    "description": "iFDO Parser Package to parse and validate the iFDO JSON Schema",
    "keywords": [
        "biigle",
        "ifdo-parser"
    ],
    "homepage": "https://github.com/biigle/ifdo-parser",
    "license": "The Unlicense",
    "type": "library",
    "authors": [
        {
            "name": "Christoph Bach",
            "email": "bach@intab.pro",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Biigle\\IfdoParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Biigle\\IfdoParser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
